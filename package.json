{
  "name": "debug-contest-platform",
  "version": "1.0.0",
  "description": "College-Level Individual Debugging Contest Platform",
  "scripts": {
    "start": "cd backend && node server.js",
    "dev": "cd backend && npx nodemon server.js",
    "client": "cd frontend && npx react-scripts start",
    "serve:network": "npm run build:frontend && npm run start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "preflight": "node scripts/start-production.js",
    "build:frontend": "cd frontend && npm run build",
    "seed": "node scripts/seedQuestions.js",
    "production": "npm run preflight && npm run start"
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  },
  "dependencies": {
    "axios": "^1.13.4",
    "dotenv": "^17.2.3",
    "mongoose": "^9.1.5"
  }
}
